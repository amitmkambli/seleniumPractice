<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="1" parallel="methods" name="Suite" data-provider-thread-count="2">
	<listeners>
		<listener class-name="Listners.ExtentListeners"></listener>
		<!-- to retry failed tests-->
		<listener class-name="Listners.AnotationTransformer"></listener>
	</listeners>
	<parameter name="browser" value="chrome"></parameter>
  <test name="Test" thread-count="3" parallel="methods">
	  <!-- thread-count="2" parallel="methods" -->
	  <!-- maven command: mvn test -DthreadCount=4 , refer pom.xml-->
    <classes>
      <class name="Tests.CatalogTest"/>
      <class name="Tests.CheckoutTest"/>
      <class name="Tests.PlaceOrderTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
